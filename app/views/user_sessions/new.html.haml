%h1 Temple Admin

- form_for @user_session, :url => login_url do |f|
  = f.error_messages
  %p
    = f.label :login
    %br
    = f.text_field :login
  %p
    = f.label :password
    %br
    = f.password_field :password
  %p
    = f.submit "Login"
  
  :javascript
    $(function() {
      $('#user_session_login').focus();
    })
