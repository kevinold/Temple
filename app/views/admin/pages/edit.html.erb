<%= javascript_include_tag :ckeditor, 'ckeditor/config' %>

<h1>Edit page</h1>

<% form_for([:admin, @page]) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', [:admin, @page] %> |
<%= link_to 'Back', admin_pages_path %>
