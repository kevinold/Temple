<h1>Listing prayerandpraises</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Email</th>
    <th>Body</th>
    <th>Published</th>
  </tr>

<% @prayerandpraises.each do |prayerandpraise| %>
  <tr>
    <td><%=h prayerandpraise.title %></td>
    <td><%=h prayerandpraise.email %></td>
    <td><%=h prayerandpraise.body %></td>
    <td><%=h prayerandpraise.published %></td>
    <td><%= link_to 'Show', :controller => 'prayerandpraises', :action => 'show', :id => prayerandpraise %></td>
    <td><%= link_to 'Edit', :controller => 'prayerandpraises', :action => 'edit', :id => prayerandpraise %></td>
    <td><%= link_to 'Destroy', prayerandpraise, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>


Unpublished
<table>
  <tr>
    <th>Title</th>
    <th>Email</th>
    <th>Body</th>
    <th>Published</th>
  </tr>

<% @unpublished.each do |up| %>
  <tr>
    <td><%=h up.title %></td>
    <td><%=h up.email %></td>
    <td><%=h up.body %></td>
    <td><%=h up.published %></td>
    <td><%= link_to 'Show', up %></td>
    <td><%= link_to 'Edit', edit_prayerandpraise_path(up) %></td>
    <td><%= link_to 'Destroy', up, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

